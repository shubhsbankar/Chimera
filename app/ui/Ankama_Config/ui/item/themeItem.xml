<Definition labelDebug="false">
	<Import url="[config.mod.template]button/simpleButton.xml" />
	<Import url="[config.mod.template]button/themeIconButton.xml" />
	<Constants>
		<Constant name="assets" value="[config.ui.skin]assets.swf|" />
		<Constant name="bitmap" value="[config.ui.skin]bitmap/" />
		<Constant name="css" value="[config.ui.skin]css/" />
		<Constant name="texture" value="[config.ui.skin]texture/" />
		
		<Constant name="upToDateColor" value="[colors.grid.bg.green]" />		
		<Constant name="notUpToDateColor" value="[colors.grid.bg.red]" />
	</Constants>
	<Container name="mainCtr">
		<Size>
			<AbsDimension x="850" y="120" />
		</Size>
		

		<mouseEnabled>true</mouseEnabled>
		<Texture name="tx_bg">
			<Size>
				<RelDimension x="1" y="1" />
			</Size>
			<visible>false</visible>
		</Texture>
		<Texture name="tx_selected">
			<Size>
				<AbsDimension x="16" y="46" />
			</Size>
			<bgColor>[colors.grid.selected]</bgColor>
			<visible>false</visible>
		</Texture>

		<Texture name="tx_previewUrl">
			<Anchors>
				<Anchor>
					<AbsDimension x="2" y="2" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="173" y="116" />
			</Size>
		</Texture>

		<Label name="lbl_name">
			<css>[local.css]title.css</css>
			<cssClass>whitebold</cssClass>
			<Anchors>
				<Anchor>
					<AbsDimension x="190" y="0" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="410" y="21" />
			</Size>
		</Label>
				
		<Label name="lbl_made">
			<css>[local.css]small2.css</css>
			<cssClass>dark</cssClass>
			<Anchors>
				<Anchor>
					<AbsDimension x="190" y="25" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="700" y="21" />
			</Size>
		</Label>
		
		<Label name="lbl_desc">
			<css>[local.css]small2.css</css>
			<cssClass>p</cssClass>
			<Anchors>
				<Anchor>
					<AbsDimension x="190" y="50" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="600" y="42" />
			</Size>
			<multiline>true</multiline>
		</Label>
		
		
		<Label name="lbl_compatibility">
			<Anchors>
				<Anchor>
					<AbsDimension x="190" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="410" y="21" />
			</Size>
			<cssClass>p</cssClass>
			<css>[local.css]normal2.css</css>
		</Label>

		<Label name="lbl_themeVersion">
			<css>[local.css]normal2.css</css>
			<cssClass>condensedboldright</cssClass>
			<Anchors>
				<Anchor>
					<AbsDimension x="695" y="6" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="120" y="21" />
			</Size>
		</Label>

		<themeIconButton name="btn_delete">
			<Anchors>
				<Anchor>
					<AbsDimension x="790" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="30" y="30" />
			</Size>
			<iconUri>[local.texture]btnIcon/btnIcon_trash.png</iconUri>
			<iconAnchors>
				<Anchor relativePoint="TOPLEFT">
					<AbsDimension x="3" y="2" />
				</Anchor>
				<Anchor relativePoint="BOTTOMRIGHT">
					<AbsDimension x="-2" y="-2" />
				</Anchor>
			</iconAnchors>
		</themeIconButton>
		<simpleButton name="btn_update" label="[ui.common.update]">
			<css>[local.css]button.css</css>
			<Anchors>
				<Anchor>
					<AbsDimension x="670" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="110" y="30" />
			</Size>
			<cssClass>small</cssClass>
		</simpleButton>
		<simpleButton name="btn_install" label="[ui.common.install]">
			<css>[local.css]button.css</css>
			<Anchors>
				<Anchor>
					<AbsDimension x="670" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="147" y="30" />
			</Size>
			<cssClass>small</cssClass>
		</simpleButton>
	</Container>
</Definition>